{
  extends: [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  env: {
    es6: true,
    browser: true,
    node: true
  },
  parserOptions: {
    ecmaVersion: 6,
    sourceType: "module",
    ecmaFeatures: {
      jsx: true
    }
  },
  globals: {
    global: true
  },
  parser: "babel-eslint",
  plugins: [
    "react"
  ]
}
